{
  "name": "fitness-tracker-2024",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "EXPO_DEVTOOLS_LISTEN_ADDRESS=localhost expo start --localhost --clear",
    "android": "EXPO_DEVTOOLS_LISTEN_ADDRESS=localhost expo start --android --localhost --clear",
    "ios": "EXPO_DEVTOOLS_LISTEN_ADDRESS=localhost expo start --ios --localhost --clear",
    "web": "EXPO_DEVTOOLS_LISTEN_ADDRESS=localhost expo start --web --localhost --clear",
    "reset": "rm -rf ~/.expo && rm -rf node_modules/.cache && npx kill-port 8081 8082"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
